{
    "plugins": [
        "node_modules/better-docs/category",
        "node_modules/better-docs/component"
    ],
    "recurseDepth": 10,
    "source": {
        "include": "src/"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "opts": {
        "template": "config/jsdoc/template",
        "destination": "./docs/",
        "readme": "./readme.md",
        "tutorials": "./src-docs/tutorials",
        "pages": "./src-docs/pages"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "better-docs": {
            "logo": "ol-kit-logo.png",
            "hideGenerator": true,
            "navigation": [
                {
                    "label": "Get Started",
                    "href": "tutorial-Getting%20Started.html"
                },
                {
                    "label": "Docs",
                    "href": "docs.html"
                },
                {
                    "label": "Github",
                    "href": "https://github.com/MonsantoCo/ol-kit"
                }
            ]
        }
    }
}
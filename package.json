  "scripts": {
    "build": "rm -rf core/ && babel src/ -d core",
    "dev": "rm -rf core/ && { babel --watch src/ -d core; } | tee >&2",
    "app:build": "cross-env-shell OL_KIT_DEVELOPMENT=true webpack --config app/webpack.config.js",
    "app": "cross-env-shell OL_KIT_DEVELOPMENT=true webpack serve --hot --inline --config app/webpack.config.js",
    "docs": "rm -rf docs/ && jsdoc -r -c jsdoc.json && cp src-docs/overrides/* docs/",
    "integrations": "./app/integration.sh",
    "lint": "eslint src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "node ./scripts/preventPublish.js",
    "ship": "node ./scripts/ship.js",
    "test": "jest --notify --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:watch": "jest --notify --config=jest.config.js --watch --coverage"
  },
